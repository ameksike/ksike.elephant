<?xml version="1.0" encoding="utf-8"?>
<config>
	<router>
		<plugins>project/testedproj/plugins/</plugins>
		<plugins>project/lightprojec/plugins/</plugins>
		<plugins>project/geomobile/plugins/</plugins>
		<lib>lib/</lib>
		<lib>project/testedproj/lib/</lib>
		<app>project/testedproj/app/</app>
		<request>
			<alias>
				<ins>
					<url>
						<pattern>params:name/params:age/params:sex/outFormat</pattern>
					</url>
					<key>
						<controller>TesIn</controller>
						<action>requestPrettyAlias</action>
						<params>
							<id>tusa</id>
						</params>
						<outFormat>json</outFormat>
						<outInfo>void</outInfo>
						<outOption>none</outOption>
					</key>
				</ins>
			</alias>
		</request>
	</router>
		<loader>
			<cfg>
				<load>auto</load>
			</cfg>
			<filter>
				<iface>
					<Module>Security</Module>
					<Module>Error</Module>
					<TesIn>Loader</TesIn>
				</iface>
				<control>
					<Module>Primal</Module>
					<Main>App</Main>
					<Plugin>Plugin</Plugin>
				</control>
				<decorate>
					<Main>TusaD</Main>
					<Main>MegaD</Main>
				</decorate>
			</filter>
		</loader>
		<package>
			<han>
				<mod>
					<proxy>Han</proxy>
				</mod>
				<cfg>
					<type>object</type>
					<load>manual</load>
				</cfg>
			</han>
			<out>
				<mod>
					<proxy>Out</proxy>
					<driver>html</driver>
					<driver>json</driver>
					<driver>img</driver>
					<driver>xml</driver>
					<base>OutDriver</base>
				</mod>
				<cfg>
					<type>object</type>
					<load>manual</load>
				</cfg>
			</out>
			<config>
				<mod>
					<proxy>Config</proxy>
					<driver>php</driver>
					<driver>json</driver>
					<driver>xml</driver>
					<driver>ini</driver>
					<base>ConfigDriver</base>
				</mod>
				<cfg>
					<type>object</type>
					<load>manual</load>
					<order>php</order>
					<order>xml</order>
					<order>ini</order>
					<order>json</order>
				</cfg>
			</config>
			<front>
				<mod>
					<proxy>ProxyFC</proxy>
					<control>OrdinaryFC</control>
					<base>FrontController</base>
				</mod>
				<cfg>
					<type>object</type>
					<load>auto</load>
				</cfg>
			</front>
			<ioc>
				<mod>
					<proxy>IOC</proxy>
					<driver>class</driver>
					<driver>object</driver>
					<driver>config</driver>
					<driver>driver</driver>
					<driver>package</driver>
					<base>IOCDriver</base>
				</mod>
				<cfg>
					<type>object</type>
					<load>manual</load>
				</cfg>
			</ioc>
		</package>
</config>
