<?xml version="1.0" encoding="utf-8"?>
<tpl>
    <info>
        <name>MobileKit</name>
        <type>project</type>
        <category>ksike</category>
        <created>10/15/2011</created>
        <author>Mr. Nosinc</author>
        <description>MobileKit blah blah..</description>
    </info>    
    <data>
        <asociations>
            <app>MobileApplet</app>
            <app>MobilePlatform</app>
        </asociations>
        <ref>
            <name>core</name>
            <val>{$GLOBALS['core']}</val>
        </ref>
        <ref>
            <name>author</name>
            <val>{$GLOBALS['vars']->author}</val>
        </ref>
        <ref>
            <name>org</name>
            <val>{$GLOBALS['vars']->org}</val>
        </ref>
        <ref>
            <name>project</name>
            <val>{$GLOBALS['name']}</val>
        </ref>
        <ref>
            <name>template</name>
            <val>
                <name>name</name>
                <val>{$GLOBALS['tpl']}</val>
            </val>
            <val>
                <name>path</name>
                <val>{$GLOBALS['tplPath']}</val>
            </val>
        </ref>
        <dir>
            <name>app</name>
            <required>true</required>
            <editable>true</editable>
            <children></children>
            <description></description>
        </dir>
        <dir>
            <name>core</name>
            <required>true</required>
            <editable>false</editable>
            <children></children>
            <description></description>
        </dir>
        <dir>
            <name>lib</name>
            <required>false</required>
            <editable>true</editable>
            <children></children>
            <description></description>
        </dir>
        <dir>
            <name>plugins</name>
            <required>true</required>
            <editable>false</editable>
            <children></children>
            <description></description>
        </dir>
        <dir>
            <name>tools</name>
            <required>false</required>
            <editable>true</editable>
            <children></children>
            <description></description>
        </dir>
        <depend>
            <name>ksike</name>
            <type>core</type>
            <version>1.1</version>
            <required>true</required>
        </depend>
        <gui>
            <type>panel</type>
            <layout>card</layout>
            <items>
                <type>panel</type>
                <layout>form</layout>
                <items>
                    <author>
                        <type>textfield</type>
                        <label>Author</label>
                        <value>Mr. Nosinc</value>
                    </author>
                    <org>
                        <type>textfield</type>
                        <label>Organization</label>
                        <value>Nosinc</value>
                    </org>
                </items>
            </items>			
        </gui>
    </data>
</tpl>
